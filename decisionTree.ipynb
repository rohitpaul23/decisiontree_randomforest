    "def oneHotCoding(feature):\n",
    "    m = len(feature)\n",
    "    discreteVal = feature.unique()\n",
    "    len_ = len(discreteVal)\n",
    "    multiArray = []\n",
    "    for i in range(m):\n",
    "        rowList = []\n",
    "        for j in range(len_):\n",
    "            if feature[i] == discreteVal[j]:\n",
    "                rowList.append(1)\n",
    "            else:\n",
    "                rowList.append(0)\n",
    "        multiArray.append(rowList)\n",
    "    #print(multiArray)\n",
    "    df = pd.DataFrame(multiArray, columns = discreteVal)\n",
    "    return df"
   ]
